{
  "name": "bpt",
  "version": "1.0.0",
  "author": "DrSparkles Warfield",
  "scripts": {
    "clear": "rimraf -rf dist",
    "lint": "eslint server.js \"src/**/*.js\"",
    "prebuild": "npm run clear",
    "build:server": "babel -d dist/server ./src/server/",
    "build:client": "webpack",
    "build": "npm run build:client && npm run build:server",
    "dev": "nodemon --ignore dist/ --ignore src/ --exec \"npm run build:server && node dist/server/index.js\"",
    "prestart": "npm run build",
    "start": "node dist/server/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "copy": "^0.3.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "input-moment": "^0.4.0",
    "mobx": "^3.6.1",
    "mobx-react": "^4.4.2",
    "mobx-react-devtools": "^4.2.15",
    "moment": "^2.20.1",
    "nodemon": "^1.11.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0",
    "url-loader": "^0.5.8"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.1",
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "lodash.times": "^4.3.2",
    "mongodb": "^3.0.3",
    "monk": "^6.0.5",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "next",
    "react-router-dom": "^4.2.2",
    "store": "^2.0.12",
    "webpack": "^2.6.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
